<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-debugler">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>Debugler file</comment>
    <icon>debugler</icon>
    <glob pattern=".dgl" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE DGL"/>
    </magic>
    <root-XML localName="dgl" />
  </mime-type>
 </mime-info>
